package main


import "fmt"
type Point struct {
    x int
    y int
}

type Rect struct {
    origin Point
    width  int
    height int
}

func main() {
    // Create Rect - NO other struct before this
    r := Rect{origin: Point{x: 1, y: 2}, width: 10, height: 5}
    
    // Immediately read fields
    fmt.Println(r.origin.x)  // should be 1
    fmt.Println(r.origin.y)  // should be 2
    fmt.Println(r.width)     // should be 10
    fmt.Println(r.height)    // should be 5
}
