package main


import "fmt"
func main() {
	n := 100
	arr := make([]int, n)
	for i := 0; i < n; i++ {
		arr[i] = i
	}
	fmt.Println(arr[0])
	fmt.Println(arr[49])
	fmt.Println(arr[50])
	fmt.Println(arr[99])
}

