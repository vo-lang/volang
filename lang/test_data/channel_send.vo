// Test: channel send/receive
package main


import "fmt"
var ch = make(chan int, 1)

func sendToChannel() {
	ch <- 42
}

func recvFromChannel() int {
	return <-ch
}

func main() {
	sendToChannel()
	val := recvFromChannel()
	assert(val == 42, "channel value should be 42")
	fmt.Println("channel send/receive test passed")
}
