package main


import "fmt"
func getSlice() []int {
	arr := [5]int{10, 20, 30, 40, 50}
	return arr[1:4]
}

func captureArray() func() int {
	arr := [3]int{100, 200, 300}
	return func() int {
		return arr[0] + arr[1] + arr[2]
	}
}

func main() {
	s := getSlice()
	fmt.Println("slice len:", len(s))
	fmt.Println("slice[0]:", s[0])
	fmt.Println("slice[1]:", s[1])
	fmt.Println("slice[2]:", s[2])

	f := captureArray()
	sum := f()
	fmt.Println("captured sum:", sum)
}
