package main

import (
	"fmt"
	"encoding/json"
)

func main() {
    // Test String()
    n1 := json.Number("42")
    s := n1.String()
    fmt.Println("String:", s)
    assert(s == "42", "String() should return '42'")
    
    // Test IsInt()
    assert(n1.IsInt() == true, "42 should be int")
    n2 := json.Number("3.14")
    assert(n2.IsInt() == false, "3.14 should not be int")
    
    // Test Int()
    i1, err := n1.Int()
    assert(err == nil, "Int() should not error")
    assert(i1 == 42, "Int() should return 42")
    fmt.Println("Int:", i1)
    
    // Test negative Int()
    n3 := json.Number("-123")
    i3, err := n3.Int()
    assert(err == nil, "Int() on negative should not error")
    assert(i3 == -123, "Int() should return -123")
    fmt.Println("Negative Int:", i3)
    
    // Test Int() on float should error
    _, err = n2.Int()
    assert(err != nil, "Int() on float should error")
    fmt.Println("Int on float error:", err)
    
    fmt.Println("All tests passed!")
}
