package main

// Test: dynamic call to function with no return value

type State struct {
	Count int
}

func increment(s *State) {
	s.Count++
}

func main() {
	var action any = increment
	var state any = &State{Count: 0}
	s := state.(*State)
	
	println("Before:", s.Count)
	
	// Dynamic call to function with no return value
	// LHS must match: no result, just error
	err := action~>(s)
	if err != nil {
		println("ERROR:", err)
	}
	
	println("After:", s.Count)
	assert(s.Count == 1)
}
