// Test dynamic access on slice with interface elements
// Bug #2: dyn_get_index returns interface element with wrong itab for any

import (
	"fmt"
	"errors"
)

func main() {
    // Create a slice of errors
    errs := []error{errors.New("err1"), errors.New("err2")}
    
    // Cast to any
    var a any = errs
    
    // Dynamic index access
    v, err := a~>[0]
    assert(err == nil, "unexpected error")
    
    // Type assert to error
    e, ok := v.(error)
    assert(ok, "type assertion to error failed")
    assert(e != nil, "error is nil")
    assert(e.Error() == "err1", "error message mismatch")
    
    fmt.Println("PASS")
}

// Expected output:
// PASS
