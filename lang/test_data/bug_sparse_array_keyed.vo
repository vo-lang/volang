// BUG: Sparse array literals with index keys don't work
// Keyed elements are placed sequentially at indices 0, 1, 2... instead of at specified indices
package main


import "fmt"
func main() {
	// This should put 20 at index 2, 50 at index 5, 90 at index 9
	arr := [10]int{2: 20, 5: 50, 9: 90}
	
	// Expected: arr[0]=0, arr[2]=20, arr[5]=50, arr[9]=90
	// Actual:   arr[0]=20, arr[1]=50, arr[2]=90 (keys ignored!)
	
	assert(arr[0] == 0, "arr[0] should be 0, got ", arr[0])
	assert(arr[2] == 20, "arr[2] should be 20, got ", arr[2])
	assert(arr[5] == 50, "arr[5] should be 50, got ", arr[5])
	assert(arr[9] == 90, "arr[9] should be 90, got ", arr[9])
	
	fmt.Println("bug_sparse_array_keyed: ok")
}
