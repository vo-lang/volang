package main


import "fmt"
// Simple function to test func JIT (no loops)
func add(a, b int) int {
	return a + b
}

func main() {
	sum := 0
	// Call add 200 times to trigger JIT (threshold=100)
	for i := 0; i < 200; i++ {
		sum = add(sum, i)
	}
	fmt.Println(sum)  // Expected: 0+1+2+...+199 = 19900
}
