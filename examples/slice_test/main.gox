package main

func main() {
    // Test break
    println("break:")
    for i := 0; i < 10; i++ {
        if i == 3 {
            break
        }
        println(i)
    }
    
    // Test continue
    println("continue:")
    for i := 0; i < 5; i++ {
        if i == 2 {
            continue
        }
        println(i)
    }
    
    println(999)
}
