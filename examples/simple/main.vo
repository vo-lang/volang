package main

func add(a int, b int) int {
    return a + b
}

func factorial(n int) int {
    if n <= 1 {
        return 1
    }
    return n * factorial(n - 1)
}

func main() {
    x := add(3, 4)
    println(x)
    
    y := factorial(5)
    println(y)
    
    for i := 1; i < 5; i = i + 1 {
        println(i)
    }
}
