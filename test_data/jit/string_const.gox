// String constant loading and operations
package main

func main() {
    s := "hello"
    assert(len(s) == 5)
    
    s2 := "world"
    assert(len(s2) == 5)
    
    // String comparison
    a := "test"
    b := "test"
    c := "other"
    assert(a == b)
    assert(a != c)
    
    // Empty string
    empty := ""
    assert(len(empty) == 0)
}
