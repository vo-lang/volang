// Simple arithmetic test for JIT compiler
package main

func add(a, b int) int {
    return a + b
}

func sub(a, b int) int {
    return a - b
}

func mul(a, b int) int {
    return a * b
}

func div(a, b int) int {
    return a / b
}

func main() {
    // Test basic arithmetic through function calls
    r1 := add(10, 20)   // 30
    r2 := sub(20, 10)   // 10
    r3 := mul(5, 6)     // 30
    r4 := div(30, 5)    // 6
    
    // Combine results to verify
    total := r1 + r2 + r3 + r4  // 30 + 10 + 30 + 6 = 76
    _ = total
}
