// Nested function calls
package main

func double(x int) int {
    return x * 2
}

func square(x int) int {
    return x * x
}

func add(a, b int) int {
    return a + b
}

func compose(x int) int {
    return double(square(x))
}

func main() {
    // Nested calls
    assert(double(square(3)) == 18)  // 3*3=9, 9*2=18
    assert(square(double(2)) == 16)  // 2*2=4, 4*4=16
    assert(add(double(3), square(2)) == 10)  // 6 + 4 = 10
    
    // Through compose
    assert(compose(2) == 8)   // 2*2=4, 4*2=8
    assert(compose(3) == 18)  // 3*3=9, 9*2=18
}
