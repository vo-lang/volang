package main

type Point struct {
    x int
    y int
}

type Rect struct {
    origin Point
    width  int
}

func main() {
    // Create rect without deep copy (just assignment)
    origin := Point{x: 1, y: 2}
    r := Rect{origin: origin, width: 10}
    
    println(r.origin.x)  // should be 1
    println(r.origin.y)  // should be 2
}
