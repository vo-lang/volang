// Test: struct embedding interface, calling embedded method through interface
// SKIP: embed interface not yet supported
package main

type Inner interface {
    Foo() int
}

type InnerImpl struct {
    val int
}

func (i *InnerImpl) Foo() int { return i.val }

type Outer struct {
    Inner
}

func main() {
    impl := &InnerImpl{val: 42}
    outer := &Outer{Inner: impl}
    
    // Direct call through embedded interface
    result := outer.Foo()
    assert(result == 42, "direct call")
    
    // Call through interface variable
    var iface Inner = outer
    result2 := iface.Foo()
    assert(result2 == 42, "interface call")
    
    println("embed_iface_simple: PASSED")
}
