package main

type Point struct {
    x int
    y int
}

type Rect struct {
    origin Point
    width  int
    height int
}

func main() {
    // Just having a Point before Rect
    p := Point{x: 100, y: 200}
    _ = p
    
    r := Rect{origin: Point{x: 1, y: 2}, width: 10, height: 5}
    println(r.origin.x)  // should be 1
    println(r.origin.y)  // should be 2
}
