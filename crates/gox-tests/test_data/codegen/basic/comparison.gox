// +vm
// Test: comparison operators
package main

func main() {
    a := 5
    b := 10
    
    // Less than
    assert(a < b, "5 < 10 should be true")
    assert(!(b < a), "10 < 5 should be false")
    
    // Greater than
    assert(b > a, "10 > 5 should be true")
    assert(!(a > b), "5 > 10 should be false")
    
    // Equal
    assert(a == 5, "5 == 5 should be true")
    assert(!(a == b), "5 == 10 should be false")
    
    // Not equal
    assert(a != b, "5 != 10 should be true")
    assert(!(a != 5), "5 != 5 should be false")
    
    // Less than or equal
    assert(a <= b, "5 <= 10 should be true")
    assert(a <= 5, "5 <= 5 should be true")
    
    // Greater than or equal
    assert(b >= a, "10 >= 5 should be true")
    assert(b >= 10, "10 >= 10 should be true")
}

