// +vm
package main

type Point struct {
    x int
    y int
}

type Rect struct {
    origin Point
    width  int
    height int
}

func main() {
    // Create Point but don't assign to blank identifier
    p := Point{x: 100, y: 200}
    println(p.x)  // 100
    
    r := Rect{origin: Point{x: 1, y: 2}, width: 10, height: 5}
    println(r.origin.x)  // should be 1
}
