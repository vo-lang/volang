package main

import "time"

func main() {
    println("Starting time tests...")
    
    // Test Now
    t1 := time.Now()
    assert(t1 > 0, "Now should return positive timestamp")
    
    // Test Since
    elapsed := time.Since(t1)
    assert(elapsed >= 0, "Since should return non-negative duration")
    
    // Test Unix
    ts := time.Unix(1000, 500)
    assert(ts == 1000000000500, "Unix should convert correctly")
    
    // Test UnixMilli
    tsm := time.UnixMilli(1000)
    assert(tsm == 1000000000, "UnixMilli should convert correctly")
    
    // Test ParseDuration
    d1 := time.ParseDuration("1s")
    assert(d1 == 1000000000, "ParseDuration 1s")
    
    d2 := time.ParseDuration("100ms")
    assert(d2 == 100000000, "ParseDuration 100ms")
    
    println("All time tests passed!")
}

