package main

import "os"

func main() {
    println("Starting os tests...")
    
    // Test Getenv
    val := os.Getenv("PATH")
    assert(len(val) > 0, "PATH should be set")
    
    // Test Getpid
    pid := os.Getpid()
    assert(pid > 0, "Getpid should return positive value")
    
    println("All os tests passed!")
}

