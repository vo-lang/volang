// Test ambiguous selector - same field name at same depth
package main

type A struct {
    x int
}

type B struct {
    x int
}

type C struct {
    A
    B
}

func main() {
    c := C{A: A{x: 1}, B: B{x: 2}}
    
    // Ambiguous: both A.x and B.x at same depth
    _ = c.x
    
    // But explicit access is OK
    _ = c.A.x
    _ = c.B.x
}

